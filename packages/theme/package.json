{
	"name": "@starter-ui/theme",
	"version": "1.0.3",
	"description": "The custom theme for Starter UI components",
	"type": "module",
	"main": "dist/index.umd.js",
	"module": "dist/index.es.js",
	"types": "dist/index.d.ts",
