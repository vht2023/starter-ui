	"author": "thien.v <thien.vh611@gmail.com>",
	"homepage": "https://starterui.vercel.app",
	"license": "MIT",
	"repository": {
		"url": "git+https://github.com/vht2023/starter-ui.git",
		"type": "git",
		"directory": "packages/theme"
	},
	"bugs": {
		"url": "https://github.com/vht2023/starter-ui/issues"
